{
  "compilerOptions": {
    "baseUrl": "./packages",
    "paths": {
      "@vimes/api": ["./graphql"]
    },
    "module": "es6",
    "target": "es5",
    "lib": [
      "es6",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "typeRoots": [
    "node_modules/@types"
  ],
  "exclude": [
    "node_modules"
  ]
}